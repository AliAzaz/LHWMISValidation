<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    tools:context=".LHWDashbord">

    <data>

        <import type="android.view.View" />

        <variable
            name="callback"
            type="com.irfansyed.umeedenau.validation.Form1SectionF" />
    </data>


    <ScrollView
        android:layout_width="match_parent"
        android:layout_height="fill_parent"
        android:layout_marginBottom="5dp"
        android:fadeScrollbars="false"
        android:fillViewport="true"
        android:paddingBottom="5dp"
        android:paddingLeft="5dp"
        android:paddingRight="5dp"
        android:paddingTop="5dp"
        android:scrollbarSize="50dip">


        <!-- Parent layout starts -->
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:background="#F1F1F1"
            android:orientation="vertical">


            <LinearLayout
                android:layout_width="fill_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="10dp">

                <ImageView
                    android:layout_width="64dp"
                    android:layout_height="64dp"
                    android:src="@drawable/uncolor" />

                <TextView
                    style="@style/TextView"
                    android:text="@string/lhwf1header_f"
                    android:layout_marginTop="20dp"
                    android:layout_marginLeft="7dp"
                    android:textColor="#008E45" />

            </LinearLayout>

            <View
                android:layout_width="match_parent"
                android:layout_height="1dp"
                android:layout_marginLeft="10dp"
                android:layout_marginRight="10dp"
                android:layout_marginBottom="10dp"
                android:background="@color/hitnscolor" />


        <LinearLayout
            android:id="@+id/SectionF"
            style="@style/Q_Linerlayout_v">

            <!-- lhwf1f1 starts -->
            <android.support.v7.widget.CardView style="@style/Q_CardView">

                <LinearLayout
                    android:id="@+id/Lv_lhwf1f1"
                    style="@style/Q_Linerlayout_v">

                    <TextView
                        style="@style/TextView"
                        android:text="@string/lhwf1f1" />

                    <com.edittextpicker.aliazaz.EditTextPicker
                        android:id="@+id/lhwf1f1"
                        style="@style/EditText_number"
                        android:inputType="number"
                        app:mask="###"
                        app:maxValue="150"
                        app:minValue="1"
                        app:type="range" />

                </LinearLayout>

            </android.support.v7.widget.CardView>
            <!-- lhwf1f1 ends-->


            <!-- lhwf1f2 starts -->
            <android.support.v7.widget.CardView style="@style/Q_CardView">

                <LinearLayout
                    android:id="@+id/Lv_lhwf1f2"
                    style="@style/Q_Linerlayout_v">

                    <TextView
                        style="@style/TextView"
                        android:text="@string/lhwf1f2"
                        android:digits="123456789" />

                    <com.edittextpicker.aliazaz.EditTextPicker
                        android:id="@+id/lhwf1f2"
                        style="@style/EditText_number"
                        android:inputType="number"
                        app:mask="###"
                        app:maxValue="150"
                        app:minValue="1"
                        app:type="range" />

                </LinearLayout>

            </android.support.v7.widget.CardView>
            <!-- lhwf1f2 ends-->


            <!-- lhwf1f3 starts -->
            <android.support.v7.widget.CardView style="@style/Q_CardView">

                <LinearLayout
                    android:id="@+id/Lv_lhwf1f3"
                    style="@style/Q_Linerlayout_v">

                    <TextView
                        style="@style/TextView"
                        android:text="@string/lhwf1f3" />

                    <com.edittextpicker.aliazaz.EditTextPicker
                        android:id="@+id/lhwf1f3"
                        style="@style/EditText_number"
                        android:inputType="number"
                        app:mask="##"
                        app:maxValue="20"
                        app:minValue="1"
                        app:type="range" />

                </LinearLayout>

            </android.support.v7.widget.CardView>
            <!-- lhwf1f3 ends-->


            <!-- lhwf1f4 starts -->
            <android.support.v7.widget.CardView style="@style/Q_CardView">

                <LinearLayout
                    android:id="@+id/Lv_lhwf1f4"
                    style="@style/Q_Linerlayout_v">

                    <TextView
                        style="@style/TextView"
                        android:text="@string/lhwf1f4" />

                    <EditText
                        style="@style/EditText_Name"
                        android:id="@+id/lhwf1f4"
                        android:maxLength="50"
                        android:hint="@string/lhwf1f4"/>

                </LinearLayout>

            </android.support.v7.widget.CardView>
            <!-- lhwf1f4 ends-->


            <!-- lhwf1f4A starts -->
            <android.support.v7.widget.CardView style="@style/Q_CardView">

                <LinearLayout
                    android:id="@+id/Lv_lhwf1f4A"
                    style="@style/Q_Linerlayout_v">

                    <TextView
                        style="@style/TextView"
                        android:text="@string/lhwf1f4A" />

                    <com.edittextpicker.aliazaz.EditTextPicker
                        android:id="@+id/lhwf1f4Aa"
                        style="@style/EditText_number"
                        android:inputType="number"
                        android:hint="month"
                        app:mask="##"
                        app:maxValue="12"
                        app:minValue="1"
                        app:type="range" />

                    <com.edittextpicker.aliazaz.EditTextPicker
                        android:id="@+id/lhwf1f4Ab"
                        style="@style/EditText_number"
                        android:inputType="number"
                        android:hint="Date"
                        app:mask="##"
                        app:maxValue="31"
                        app:minValue="1"
                        app:type="range" />

                </LinearLayout>

            </android.support.v7.widget.CardView>
            <!-- lhwf1f4A ends-->


            <!-- lhwf1f5 starts -->
            <android.support.v7.widget.CardView style="@style/Q_CardView">

                <LinearLayout
                    android:id="@+id/Lv_lhwf1f5"
                    style="@style/Q_Linerlayout_v">

                    <TextView
                        style="@style/TextView"
                        android:text="@string/lhwf1f5" />

                    <com.edittextpicker.aliazaz.EditTextPicker
                        android:id="@+id/lhwf1f5"
                        style="@style/EditText_number"
                        android:inputType="number"
                        app:mask="##"
                        app:maxValue="59"
                        app:minValue="0"
                        app:type="range" />

                </LinearLayout>

            </android.support.v7.widget.CardView>
            <!-- lhwf1f5 ends-->


            <!-- lhwf1f6 starts -->
            <android.support.v7.widget.CardView style="@style/Q_CardView">

                <LinearLayout
                    android:id="@+id/Lv_lhwf1f6"
                    style="@style/Q_Linerlayout_v">

                    <TextView
                        style="@style/TextView"
                        android:text="@string/lhwf1f6" />

                    <RadioGroup
                        android:id="@+id/lhwf1f6"
                        style="@style/RadioGroup">

                        <RadioButton
                            android:id="@+id/lhwf1f6_1"
                            style="@style/RadioButton"
                            android:text="@string/rb_yes"/>

                        <RadioButton
                            android:id="@+id/lhwf1f6_2"
                            style="@style/RadioButton"
                            android:text="@string/rb_no"/>
                        <RadioButton
                            android:id="@+id/lhwf1f6_3"
                            style="@style/RadioButton"
                            android:text="@string/lhw_stockout"/>

                    </RadioGroup>

                </LinearLayout>

            </android.support.v7.widget.CardView>
            <!-- lhwf1f6 ends-->


            <!-- lhwf1f7 starts -->
            <android.support.v7.widget.CardView style="@style/Q_CardView">

                <LinearLayout
                    android:id="@+id/Lv_lhwf1f7"
                    style="@style/Q_Linerlayout_v">

                    <TextView
                        style="@style/TextView"
                        android:text="@string/lhwf1f7" />

                    <RadioGroup
                        android:id="@+id/lhwf1f7"
                        style="@style/RadioGroup">

                        <RadioButton
                            android:id="@+id/lhwf1f7_1"
                            style="@style/RadioButton"
                            android:text="@string/rb_yes"/>

                        <RadioButton
                            android:id="@+id/lhwf1f7_2"
                            style="@style/RadioButton"
                            android:text="@string/rb_no"/>
                        <RadioButton
                            android:id="@+id/lhwf1f7_3"
                            style="@style/RadioButton"
                            android:text="@string/lhw_stockout"/>

                    </RadioGroup>

                </LinearLayout>

            </android.support.v7.widget.CardView>
            <!-- lhwf1f7 ends-->


            <!-- lhwf1f8 starts -->
            <android.support.v7.widget.CardView style="@style/Q_CardView">

                <LinearLayout
                    android:id="@+id/Lv_lhwf1f8"
                    style="@style/Q_Linerlayout_v">

                    <TextView
                        style="@style/TextView"
                        android:text="@string/lhwf1f8" />

                    <RadioGroup
                        android:id="@+id/lhwf1f8"
                        style="@style/RadioGroup">

                        <RadioButton
                            android:id="@+id/lhwf1f8_1"
                            style="@style/RadioButton"
                            android:text="@string/rb_yes"/>

                        <RadioButton
                            android:id="@+id/lhwf1f8_2"
                            style="@style/RadioButton"
                            android:text="@string/rb_no"/>

                        <RadioButton
                            android:id="@+id/lhwf1f8_3"
                            style="@style/RadioButton"
                            android:text="@string/rb_na"/>

                    </RadioGroup>

                </LinearLayout>

            </android.support.v7.widget.CardView>
            <!-- lhwf1f8 ends-->


            <!-- lhwf1f9 starts -->
            <android.support.v7.widget.CardView style="@style/Q_CardView">

                <LinearLayout
                    android:id="@+id/Lv_lhwf1f9"
                    style="@style/Q_Linerlayout_v">

                    <TextView
                        style="@style/TextView"
                        android:text="@string/lhwf1f9" />

                    <RadioGroup
                        android:id="@+id/lhwf1f9"
                        style="@style/RadioGroup">

                        <RadioButton
                            android:id="@+id/lhwf1f9_1"
                            style="@style/RadioButton"
                            android:text="@string/rb_yes"/>

                        <RadioButton
                            android:id="@+id/lhwf1f9_2"
                            style="@style/RadioButton"
                            android:text="@string/rb_no"/>

                    </RadioGroup>

                </LinearLayout>

            </android.support.v7.widget.CardView>
            <!-- lhwf1f9 ends-->


            <!-- lhwf1f10 starts -->
            <android.support.v7.widget.CardView style="@style/Q_CardView">

                <LinearLayout
                    android:id="@+id/Lv_lhwf1f10"
                    style="@style/Q_Linerlayout_v">

                    <TextView
                        style="@style/TextView"
                        android:text="@string/lhwf1f10" />

                    <RadioGroup
                        android:id="@+id/lhwf1f10"
                        style="@style/RadioGroup">

                        <RadioButton
                            android:id="@+id/lhwf1f10_1"
                            style="@style/RadioButton"
                            android:text="@string/rb_yes"/>

                        <RadioButton
                            android:id="@+id/lhwf1f10_2"
                            style="@style/RadioButton"
                            android:text="@string/rb_no"/>

                    </RadioGroup>

                </LinearLayout>

            </android.support.v7.widget.CardView>
            <!-- lhwf1f8 ends-->


        </LinearLayout>

            <!-- Button Next Starts-->
            <android.support.v7.widget.CardView style="@style/Q_CardView">

                <Button
                    android:id="@+id/btn_next"
                    style="@style/Button_Next"
                    android:layout_margin="10dp"
                    android:layout_weight="50"
                    android:text="Submit " />

            </android.support.v7.widget.CardView>

            <!-- Button Next Ends-->


        </LinearLayout>

    </ScrollView>


</layout>
