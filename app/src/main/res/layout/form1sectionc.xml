<layout xmlns:app="http://schemas.android.com/apk/res-auto">

    <data>

        <import type="android.view.View" />

        <variable
            name="callback"
            type="com.irfansyed.umeedenau.validation.Form1SectionC" />
    </data>


    <ScrollView xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:tools="http://schemas.android.com/tools"
        android:layout_width="match_parent"
        android:layout_height="fill_parent"
        android:layout_marginBottom="5dp"
        android:fadeScrollbars="false"
        android:fillViewport="true"
        android:paddingLeft="5dp"
        android:paddingTop="5dp"
        android:paddingRight="5dp"
        android:paddingBottom="5dp"
        android:scrollbarSize="50dip"
        tools:context=".LHWDashbord">

        <!-- Parent layout starts -->
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:background="#F1F1F1"
            android:orientation="vertical">

            <LinearLayout
                android:layout_width="fill_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="10dp">

                <ImageView
                    android:layout_width="64dp"
                    android:layout_height="64dp"
                    android:src="@drawable/uncolor" />

                <TextView
                    style="@style/TextView"
                    android:layout_marginLeft="7dp"
                    android:layout_marginTop="20dp"
                    android:text="@string/lhwf1header_c"
                    android:textColor="#008E45" />

            </LinearLayout>

            <View
                android:layout_width="match_parent"
                android:layout_height="1dp"
                android:layout_marginLeft="10dp"
                android:layout_marginRight="10dp"
                android:layout_marginBottom="10dp"
                android:background="@color/hitnscolor" />

            <LinearLayout
                android:id="@+id/pp"
                style="@style/Q_Linerlayout_v">


                <!-- lhwf1c1 starts -->
                <android.support.v7.widget.CardView
                    android:id="@+id/cvlhwf1c1"
                    style="@style/Q_CardView">

                    <LinearLayout
                        android:id="@+id/Lv_lhwf1c1"
                        style="@style/Q_Linerlayout_v">

                        <TextView
                            style="@style/TextView"
                            android:text="@string/lhwf1c1" />

                        <com.edittextpicker.aliazaz.EditTextPicker
                            android:id="@+id/lhwf1c1"
                            style="@style/EditText_number"
                            android:hint="@string/num"
                            android:maxLength="2"
                            app:maxValue="50"
                            app:minValue="1"
                            app:type="range" />

                    </LinearLayout>

                </android.support.v7.widget.CardView>
                <!-- lhwf1c1 ends-->


                <!-- lhwf1c2 starts -->
                <android.support.v7.widget.CardView
                    android:id="@+id/cvlhwf1c2"
                    style="@style/Q_CardView">

                    <LinearLayout
                        android:id="@+id/Lv_lhwf1c2"
                        style="@style/Q_Linerlayout_v">

                        <TextView
                            style="@style/TextView"
                            android:text="@string/lhwf1c2" />

                        <com.edittextpicker.aliazaz.EditTextPicker
                            android:id="@+id/lhwf1c2"
                            style="@style/EditText_number"
                            android:hint="@string/num"
                            android:maxLength="1"
                            app:maxValue="5"
                            app:minValue="1"
                            app:type="range" />

                    </LinearLayout>

                </android.support.v7.widget.CardView>
                <!-- lhwf1c2 ends-->


                <!-- lhwf1c3 starts -->
                <android.support.v7.widget.CardView
                    android:id="@+id/cvlhwf1c3"
                    style="@style/Q_CardView">

                    <LinearLayout
                        android:id="@+id/Lv_lhwf1c3"
                        style="@style/Q_Linerlayout_v">

                        <TextView
                            style="@style/TextView"
                            android:text="@string/lhwf1c3" />

                        <EditText
                            android:id="@+id/lhwf1c3"
                            style="@style/EditText_Name"
                            android:hint="@string/name"
                            android:maxLength="50" />

                    </LinearLayout>

                </android.support.v7.widget.CardView>
                <!-- lhwf1c3 ends-->


                <!-- lhwf1c3a starts -->
                <android.support.v7.widget.CardView
                    android:id="@+id/cvlhwf1c3A"
                    style="@style/Q_CardView">

                    <LinearLayout
                        android:id="@+id/Lv_lhwf1c3A"
                        style="@style/Q_Linerlayout_v">

                        <TextView
                            style="@style/TextView"
                            android:text="@string/lhwf1c3A" />

                        <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:orientation="horizontal">

                            <com.edittextpicker.aliazaz.EditTextPicker
                                android:id="@+id/lhwf1c3Aa"
                                style="@style/EditText_number"
                                android:layout_width="0dp"
                                android:layout_weight="1"
                                android:hint="Month"
                                app:mask="##"
                                app:maxValue="12"
                                app:minValue="1"
                                app:type="range" />

                            <com.edittextpicker.aliazaz.EditTextPicker
                                android:id="@+id/lhwf1c3Ab"
                                style="@style/EditText_number"
                                android:layout_width="0dp"
                                android:layout_weight="1"
                                android:hint="Date"
                                app:mask="##"
                                app:maxValue="31"
                                app:minValue="1"
                                app:type="range" />

                        </LinearLayout>

                    </LinearLayout>

                </android.support.v7.widget.CardView>
                <!-- lhwf1c3a ends-->


                <!-- lhwf1c4 starts -->
                <android.support.v7.widget.CardView
                    android:id="@+id/cvlhwf1c4"
                    style="@style/Q_CardView">

                    <LinearLayout
                        android:id="@+id/Lv_lhwf1c4"
                        style="@style/Q_Linerlayout_v">

                        <TextView
                            style="@style/TextView"
                            android:text="@string/lhwf1c4" />

                        <EditText
                            android:id="@+id/lhwf1c4"
                            style="@style/EditText_Name"
                            android:hint="@string/name"
                            android:maxLength="50" />

                    </LinearLayout>

                </android.support.v7.widget.CardView>
                <!-- lhwf1c4 ends-->


                <!-- lhwf1c4a starts -->
                <android.support.v7.widget.CardView
                    android:id="@+id/cvlhwf1c4a"
                    style="@style/Q_CardView">

                    <LinearLayout
                        android:id="@+id/Lv_lhwf1c4a"
                        style="@style/Q_Linerlayout_v">

                        <TextView
                            style="@style/TextView"
                            android:text="@string/lhwf1c4A" />

                        <com.edittextpicker.aliazaz.EditTextPicker
                            android:id="@+id/lhwf1c4A"
                            style="@style/EditText_number"
                            android:hint="@string/months"
                            android:maxLength="1"
                            app:maxValue="9"
                            app:minValue="1"
                            app:type="range" />

                    </LinearLayout>

                </android.support.v7.widget.CardView>
                <!-- lhwf1c4a ends-->


                <!-- lhwf1c5 starts -->
                <android.support.v7.widget.CardView
                    android:id="@+id/cvlhwf1c5"
                    style="@style/Q_CardView">

                    <LinearLayout
                        android:id="@+id/Lv_lhwf1c5"
                        style="@style/Q_Linerlayout_v">

                        <TextView
                            style="@style/TextView"
                            android:text="@string/lhwf1c5" />

                        <com.edittextpicker.aliazaz.EditTextPicker
                            android:id="@+id/lhwf1c5"
                            style="@style/EditText_number"
                            android:hint="@string/num"
                            android:maxLength="1"
                            app:maxValue="9"
                            app:minValue="0"
                            app:type="range" />

                    </LinearLayout>

                </android.support.v7.widget.CardView>
                <!-- lhwf1c5 ends-->


                <!-- lhwf1c6 starts -->
                <android.support.v7.widget.CardView
                    android:id="@+id/cvlhwf1c6"
                    style="@style/Q_CardView">

                    <LinearLayout
                        android:id="@+id/Lv_lhwf1c6"
                        style="@style/Q_Linerlayout_v">

                        <TextView
                            style="@style/TextView"
                            android:text="@string/lhwf1c6" />

                        <RadioGroup
                            android:id="@+id/lhwf1c6"
                            style="@style/RadioGroup"
                            android:orientation="horizontal">

                            <RadioButton
                                android:id="@+id/lhwf1c6_1"
                                style="@style/RadioButton"
                                android:layout_width="0dp"
                                android:layout_weight="1"
                                android:text="@string/rb_yes" />

                            <RadioButton
                                android:id="@+id/lhwf1c6_2"
                                style="@style/RadioButton"
                                android:layout_width="0dp"
                                android:layout_weight="1"
                                android:text="@string/rb_no" />

                            <RadioButton
                                android:id="@+id/lhwf1c6_3"
                                style="@style/RadioButton"
                                android:layout_width="0dp"
                                android:layout_weight="1"
                                android:text="Not Authorized"
                                android:visibility="gone" />

                        </RadioGroup>

                    </LinearLayout>

                </android.support.v7.widget.CardView>
                <!-- lhwf1c6 ends-->


                <!-- lhwf1c7 starts -->
                <android.support.v7.widget.CardView
                    android:id="@+id/cvlhwf1c7"
                    style="@style/Q_CardView"
                    android:visibility='@{lhwf1c61.checked? View.GONE : View.VISIBLE}'>

                    <LinearLayout
                        android:id="@+id/Lv_lhwf1c7"
                        style="@style/Q_Linerlayout_v">

                        <TextView
                            style="@style/TextView"
                            android:text="@string/lhwf1c7" />

                        <RadioGroup
                            android:id="@+id/lhwf1c7"
                            style="@style/RadioGroup"
                            android:orientation="horizontal">

                            <RadioButton
                                android:id="@+id/lhwf1c7_1"
                                style="@style/RadioButton"
                                android:layout_width="0dp"
                                android:layout_weight="1"
                                android:text="@string/rb_yes" />

                            <RadioButton
                                android:id="@+id/lhwf1c7_2"
                                style="@style/RadioButton"
                                android:layout_width="0dp"
                                android:layout_weight="1"
                                android:text="@string/rb_no" />

                        </RadioGroup>

                    </LinearLayout>

                </android.support.v7.widget.CardView>
                <!-- lhwf1c7 ends-->


                <!-- lhwf1c7A starts -->
                <android.support.v7.widget.CardView
                    android:id="@+id/cvlhwf1c7A"
                    style="@style/Q_CardView"
                    android:visibility='@{lhwf1c61.checked? View.GONE : View.VISIBLE}'>

                    <LinearLayout
                        android:id="@+id/Lv_lhwf1c7A"
                        style="@style/Q_Linerlayout_v">

                        <TextView
                            style="@style/TextView"
                            android:text="@string/lhwf1c7A" />

                        <RadioGroup
                            android:id="@+id/lhwf1c7A"
                            style="@style/RadioGroup"
                            android:orientation="horizontal">

                            <RadioButton
                                android:id="@+id/lhwf1c7A_1"
                                style="@style/RadioButton"
                                android:layout_width="0dp"
                                android:layout_weight="1"
                                android:text="@string/lhwf1c7Aa" />

                            <RadioButton
                                android:id="@+id/lhwf1c7A_2"
                                style="@style/RadioButton"
                                android:layout_width="0dp"
                                android:layout_weight="1"
                                android:text="@string/lhwf1c7Ab" />

                        </RadioGroup>

                    </LinearLayout>

                </android.support.v7.widget.CardView>
                <!-- lhwf1c7A ends-->


                <!-- lhwf1c8 starts -->
                <android.support.v7.widget.CardView
                    android:id="@+id/cvlhwf1c8"
                    style="@style/Q_CardView">

                    <LinearLayout
                        android:id="@+id/Lv_lhwf1c8"
                        style="@style/Q_Linerlayout_v">

                        <TextView
                            style="@style/TextView"
                            android:text="@string/lhwf1c8" />

                        <RadioGroup
                            android:id="@+id/lhwf1c8"
                            style="@style/RadioGroup"
                            android:orientation="horizontal">

                            <RadioButton
                                android:id="@+id/lhwf1c8_1"
                                style="@style/RadioButton"
                                android:layout_width="0dp"
                                android:layout_weight="1"
                                android:text="@string/rb_yes" />

                            <RadioButton
                                android:id="@+id/lhwf1c8_2"
                                style="@style/RadioButton"
                                android:layout_width="0dp"
                                android:layout_weight="1"
                                android:text="@string/rb_no" />

                            <RadioButton
                                android:id="@+id/lhwf1c8_3"
                                style="@style/RadioButton"
                                android:layout_width="0dp"
                                android:layout_weight="1"
                                android:text="@string/lhw_stockout"
                                android:visibility="gone" />

                        </RadioGroup>

                    </LinearLayout>

                </android.support.v7.widget.CardView>
                <!-- lhwf1c8 ends-->


                <!-- lhwf1c9 starts -->
                <android.support.v7.widget.CardView
                    android:id="@+id/cvlhwf1c9"
                    style="@style/Q_CardView"
                    android:visibility='@{lhwf1c81.checked? View.GONE : View.VISIBLE}'>

                    <LinearLayout
                        android:id="@+id/Lv_lhwf1c9"
                        style="@style/Q_Linerlayout_v">

                        <TextView
                            style="@style/TextView"
                            android:text="@string/lhwf1c9" />

                        <RadioGroup
                            android:id="@+id/lhwf1c9"
                            style="@style/RadioGroup">

                            <RadioButton
                                android:id="@+id/lhwf1c9_1"
                                style="@style/RadioButton"
                                android:text="@string/lhwf1c9_1" />

                            <RadioButton
                                android:id="@+id/lhwf1c9_2"
                                style="@style/RadioButton"
                                android:text="@string/lhwf1c9_2" />


                            <RadioButton
                                android:id="@+id/lhwf1c9_3"
                                style="@style/RadioButton"
                                android:text="@string/lhwf1c9_3" />

                        </RadioGroup>

                    </LinearLayout>

                </android.support.v7.widget.CardView>
                <!-- lhwf1c9 ends-->


                <!-- lhwf1c10 starts -->
                <android.support.v7.widget.CardView
                    android:id="@+id/cvlhwf1c10"
                    style="@style/Q_CardView">

                    <LinearLayout
                        android:id="@+id/Lv_lhwf1c10"
                        style="@style/Q_Linerlayout_v">

                        <TextView
                            style="@style/TextView"
                            android:text="@string/lhwf1c10" />

                        <RadioGroup
                            android:id="@+id/lhwf1c10"
                            style="@style/RadioGroup"
                            android:orientation="horizontal">

                            <RadioButton
                                android:id="@+id/lhwf1c10_1"
                                style="@style/RadioButton"
                                android:layout_width="0dp"
                                android:layout_weight="1"
                                android:text="@string/rb_yes" />

                            <RadioButton
                                android:id="@+id/lhwf1c10_2"
                                style="@style/RadioButton"
                                android:layout_width="0dp"
                                android:layout_weight="1"
                                android:text="@string/rb_no" />

                        </RadioGroup>

                    </LinearLayout>

                </android.support.v7.widget.CardView>
                <!-- lhwf1c10 ends-->

                <!-- Button -->
                <android.support.v7.widget.CardView style="@style/Q_CardView">

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="wrap_content"
                        android:orientation="horizontal">

                        <Button
                            android:id="@+id/btn_snap"
                            style="@style/Button_Next"
                            android:layout_width="0dp"
                            android:layout_weight="1"
                            android:text="Snap Shot" />

                        <TextView
                            android:id="@+id/tv1"
                            style="@style/TextView_Header"
                            android:layout_width="0dp"
                            android:layout_weight="1"
                            android:text="Count" />

                    </LinearLayout>

                </android.support.v7.widget.CardView>


            </LinearLayout>

            <!-- Button -->
            <android.support.v7.widget.CardView style="@style/Q_CardView">

                <Button
                    android:id="@+id/btn_next"
                    style="@style/Button_Next"
                    android:layout_margin="10dp"
                    android:layout_weight="50"
                    android:text="Submit" />

            </android.support.v7.widget.CardView>
            <!-- Button Next Ends-->

        </LinearLayout>

    </ScrollView>

</layout>