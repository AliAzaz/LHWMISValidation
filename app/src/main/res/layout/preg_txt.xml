                >


                <LinearLayout

                    style="@style/Q_Linerlayout_v"

                    android:id="@+id/Lv_P25"
                    >

                    <TextView


                        style="@style/TextView"
                        android:text="Weakness (Pale skin)/Anemia "

                        />


                    <EditText
                        style="@style/EditText_varchar"

                        android:id="@+id/txt_P25"
                        />




                </LinearLayout>
            </android.support.v7.widget.CardView>
            <!-- P25 Ends-->



            <!-- P26 -->
