                >


                <LinearLayout

                    style="@style/Q_Linerlayout_v"

                    android:id="@+id/Lv_P26"
                    >

                    <TextView


                        style="@style/TextView"
                        android:text="Premature water breaking less then 37th weeks"

                        />


                    <EditText
                        style="@style/EditText_varchar"

                        android:id="@+id/txt_P26"
                        />




                </LinearLayout>
            </android.support.v7.widget.CardView>
            <!-- P26 Ends-->



            <!-- P27 -->
            <android.support.v7.widget.CardView style="@style/Q_CardView"

                >


                <LinearLayout

                    style="@style/Q_Linerlayout_v"

                    android:id="@+id/Lv_P27"
                    >

                    <TextView


                        style="@style/TextView"
                        android:text="Lower abdominal pain"

                        />


                    <EditText
                        style="@style/EditText_varchar"

                        android:id="@+id/txt_P27"
                        />




                </LinearLayout>
            </android.support.v7.widget.CardView>
            <!-- P27 Ends-->



            <!-- Button Next Starts-->
            <android.support.v7.widget.CardView
                style="@style/Q_CardView"
                >

                <Button
                    android:id="@+id/btn_preg_next"
                    style="@style/Button_Next"
                    android:layout_margin="10dp"


                    android:text="Next" />
            </android.support.v7.widget.CardView>

            <!-- Button Next Ends-->


        </LinearLayout>

    </ScrollView>


</LinearLayout>
