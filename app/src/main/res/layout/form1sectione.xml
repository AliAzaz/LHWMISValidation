<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    tools:context=".LHWDashbord">

    <data>

        <import type="android.view.View" />

        <variable
            name="callback"
            type="com.irfansyed.umeedenau.validation.Form1SectionE" />
    </data>



    <ScrollView
        android:layout_width="match_parent"
        android:layout_height="fill_parent"
        android:layout_marginBottom="5dp"
        android:fadeScrollbars="false"
        android:fillViewport="true"
        android:paddingBottom="5dp"
        android:paddingLeft="5dp"
        android:paddingRight="5dp"
        android:paddingTop="5dp"
        android:scrollbarSize="50dip">
        <!-- Parent layout starts -->
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:background="#F1F1F1"
            android:orientation="vertical">




            <LinearLayout
                android:layout_width="fill_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="10dp"
                >
                <ImageView
                    android:layout_width="64dp"
                    android:layout_height="64dp"
                    android:src="@drawable/uncolor"
                    />
                <TextView
                    style="@style/TextView"
                    android:text="@string/lhwf1header_e"
                    android:layout_marginTop="20dp"
                    android:layout_marginLeft="7dp"
                    android:textColor="#008E45"

                    />

            </LinearLayout>
            <View
                android:layout_width="match_parent"
                android:layout_height="1dp"
                android:layout_marginLeft="10dp"
                android:layout_marginRight="10dp"
                android:layout_marginBottom="10dp"
                android:background="@color/hitnscolor" />



        <LinearLayout
            android:id="@+id/SectionE"
            style="@style/Q_Linerlayout_v">


            <!-- lhwf1e1 starts -->
            <android.support.v7.widget.CardView style="@style/Q_CardView"


                >



                <LinearLayout
                    android:id="@+id/Lv_lhwf1e1"
                    style="@style/Q_Linerlayout_v"

                    >

                    <TextView

                        style="@style/TextView"
                        android:text="@string/lhwf1e1"

                        />

                    <EditText
                        style="@style/EditText_number"
                        android:id="@+id/lhwf1e1"
                        android:maxLength="3"
                        android:hint="@string/lhwf1e1"/>


                </LinearLayout>
            </android.support.v7.widget.CardView>
            <!-- lhwf1e1 ends-->


            <!-- lhwf1e2 starts -->
            <android.support.v7.widget.CardView style="@style/Q_CardView"


                >



                <LinearLayout
                    android:id="@+id/Lv_lhwf1e2"
                    style="@style/Q_Linerlayout_v"

                    >

                    <TextView

                        style="@style/TextView"
                        android:text="@string/lhwf1e2"

                        />

                    <EditText
                        style="@style/EditText_number"
                        android:id="@+id/lhwf1e2"
                        android:maxLength="3"
                        android:hint="@string/lhwf1e2"/>


                </LinearLayout>
            </android.support.v7.widget.CardView>
            <!-- lhwf1e2 ends-->

            <!-- lhwf1e3 starts -->
            <android.support.v7.widget.CardView style="@style/Q_CardView"


                >



                <LinearLayout
                    android:id="@+id/Lv_lhwf1e3"
                    style="@style/Q_Linerlayout_v"

                    >

                    <TextView

                        style="@style/TextView"
                        android:text="@string/lhwf1e3"

                        />

                    <EditText
                        style="@style/EditText_Name"
                        android:id="@+id/lhwf1e3"
                        android:maxLength="50"
                        android:hint="@string/lhwf1e3"/>


                </LinearLayout>
            </android.support.v7.widget.CardView>
            <!-- lhwf1e3 ends-->

            <!-- lhwf1e4 starts -->
            <android.support.v7.widget.CardView style="@style/Q_CardView"


                >



                <LinearLayout
                    android:id="@+id/Lv_lhwf1e4"
                    style="@style/Q_Linerlayout_v"

                    >

                    <TextView

                        style="@style/TextView"
                        android:text="@string/lhwf1e4"

                        />

                    <EditText
                        style="@style/EditText_Name"
                        android:id="@+id/lhwf1e4"
                        android:maxLength="50"
                        android:hint="@string/lhwf1e4"/>


                </LinearLayout>
            </android.support.v7.widget.CardView>
            <!-- lhwf1e4 ends-->



            <!-- lhwf1e5 starts -->
            <android.support.v7.widget.CardView style="@style/Q_CardView"


                >



                <LinearLayout
                    android:id="@+id/Lv_lhwf1e5"
                    style="@style/Q_Linerlayout_v"

                    >

                    <TextView

                        style="@style/TextView"
                        android:text="@string/lhwf1e5"

                        />

                    <RadioGroup
                        android:id="@+id/lhwf1e5"
                        style="@style/RadioGroup">

                        <RadioButton
                            android:id="@+id/lhwf1e5_1"
                            style="@style/RadioButton"
                            android:text="@string/rb_yes"/>

                        <RadioButton
                            android:id="@+id/lhwf1e5_2"
                            style="@style/RadioButton"
                            android:text="@string/rb_no"/>
                        <RadioButton
                            android:id="@+id/lhwf1e5_3"
                            style="@style/RadioButton"
                            android:text="Weight machine not working/available"/>


                    </RadioGroup>

                </LinearLayout>
            </android.support.v7.widget.CardView>
            <!-- lhwf1e5 ends-->


            <!-- lhwf1e6 starts -->
            <android.support.v7.widget.CardView style="@style/Q_CardView"


                >



                <LinearLayout
                    android:id="@+id/Lv_lhwf1e6"
                    style="@style/Q_Linerlayout_v"

                    >

                    <TextView

                        style="@style/TextView"
                        android:text="@string/lhwf1e6"

                        />

                    <RadioGroup
                        android:id="@+id/lhwf1e6"
                        style="@style/RadioGroup">

                        <RadioButton
                            android:id="@+id/lhwf1e6_1"
                            style="@style/RadioButton"
                            android:text="@string/rb_yes"/>

                        <RadioButton
                            android:id="@+id/lhwf1e6_2"
                            style="@style/RadioButton"
                            android:text="@string/rb_no"/>

                        <RadioButton
                            android:id="@+id/lhwf1e6_3"
                            style="@style/RadioButton"
                            android:text="@string/rb_na"/>


                    </RadioGroup>

                </LinearLayout>
            </android.support.v7.widget.CardView>
            <!-- lhwf1e6 ends-->

            <!-- lhwf1e7 starts -->
            <android.support.v7.widget.CardView style="@style/Q_CardView"


                >



                <LinearLayout
                    android:id="@+id/Lv_lhwf1e7"
                    style="@style/Q_Linerlayout_v"

                    >

                    <TextView

                        style="@style/TextView"
                        android:text="@string/lhwf1e7"

                        />

                    <RadioGroup
                        android:id="@+id/lhwf1e7"
                        style="@style/RadioGroup">

                        <RadioButton
                            android:id="@+id/lhwf1e7_1"
                            style="@style/RadioButton"
                            android:text="@string/rb_yes"/>

                        <RadioButton
                            android:id="@+id/lhwf1e7_2"
                            style="@style/RadioButton"
                            android:text="@string/rb_no"/>



                    </RadioGroup>

                </LinearLayout>
            </android.support.v7.widget.CardView>
            <!-- lhwf1e7 ends-->

            <!-- lhwf1e8 starts -->
            <android.support.v7.widget.CardView style="@style/Q_CardView"


                >



                <LinearLayout
                    android:id="@+id/Lv_lhwf1e8"
                    style="@style/Q_Linerlayout_v"

                    >

                    <TextView

                        style="@style/TextView"
                        android:text="@string/lhwf1e8"

                        />

                    <RadioGroup
                        android:id="@+id/lhwf1e8"
                        style="@style/RadioGroup">

                        <RadioButton
                            android:id="@+id/lhwf1e8_1"
                            style="@style/RadioButton"
                            android:text="@string/rb_yes"/>

                        <RadioButton
                            android:id="@+id/lhwf1e8_2"
                            style="@style/RadioButton"
                            android:text="@string/rb_no"/>




                    </RadioGroup>

                </LinearLayout>
            </android.support.v7.widget.CardView>
            <!-- lhwf1e8 ends-->




</LinearLayout>

            <!-- Button -->
            <android.support.v7.widget.CardView
                style="@style/Q_CardView"

                >




                <Button
                    android:id="@+id/btn_Next"
                    style="@style/Button_Next"
                    android:layout_margin="10dp"
                    android:layout_weight="50"
                    android:text="Submit " />



            </android.support.v7.widget.CardView>

            <!-- Button Next Ends-->


        </LinearLayout>

    </ScrollView>


</layout>
