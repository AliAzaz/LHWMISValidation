                        <RadioButton
                            android:id="@+id/lhwf2c8_1"
                            style="@style/RadioButton"
                            android:text="@string/rb_yes"/>

                        <RadioButton
                            android:id="@+id/lhwf2c8_2"
                            style="@style/RadioButton"
                            android:text="@string/rb_no"/>
                        <RadioButton
                            android:visibility="gone"
                            android:id="@+id/lhwf2c8_3"
                            style="@style/RadioButton"
                            android:text="@string/lhw_stockout"/>



                    </RadioGroup>


                </LinearLayout>
            </android.support.v7.widget.CardView>
            <!-- lhwf2c8 ends-->



            <!-- lhwf2c8a starts -->
            <android.support.v7.widget.CardView style="@style/Q_CardView"


                >



                <LinearLayout
                    android:id="@+id/Lv_lhwf2c8a"
                    style="@style/Q_Linerlayout_v"

                    >

                    <TextView

                        style="@style/TextView"
                        android:text="Any maternal death (during pregnancy) occurred in last three months?"

                        />
                    <RadioGroup
                        android:id="@+id/lhwf2c8a"
                        style="@style/RadioGroup">

                        <RadioButton
                            android:id="@+id/lhwf2c8a_1"
                            style="@style/RadioButton"
                            android:text="@string/rb_yes"/>

                        <RadioButton
                            android:id="@+id/lhwf2c8a_2"
                            style="@style/RadioButton"
                            android:text="@string/rb_no"/>




                    </RadioGroup>


                </LinearLayout>
            </android.support.v7.widget.CardView>
            <!-- lhwf2c8a ends-->

            <!-- Quality Indicators starts -->
            <android.support.v7.widget.CardView style="@style/Q_CardView"


                >



                <LinearLayout

                    style="@style/Q_Linerlayout_v"
                    android:background="@color/colorPrimary"

                    >

                    <TextView

                        style="@style/TextView"
                        android:text="Quality Indicators"
                        android:textColor="@color/white"

                        />


                </LinearLayout>
            </android.support.v7.widget.CardView>
            <!-- Quality Indicators ends-->



            <!-- lhwf2c9 starts -->
            <android.support.v7.widget.CardView style="@style/Q_CardView"


                >



                <LinearLayout
                    android:id="@+id/Lv_lhwf2c9"
