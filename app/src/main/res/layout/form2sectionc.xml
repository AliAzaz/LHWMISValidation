<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    tools:context=".LHWDashbord">

    <data>

        <import type="android.view.View" />

        <variable
            name="callback"
            type="com.irfansyed.umeedenau.validation.Form2SectionC" />
    </data>


    <ScrollView
        android:layout_width="match_parent"
        android:layout_height="fill_parent"
        android:layout_marginBottom="5dp"
        android:fadeScrollbars="false"
        android:fillViewport="true"
        android:paddingBottom="5dp"
        android:paddingLeft="5dp"
        android:paddingRight="5dp"
        android:paddingTop="5dp"
        android:scrollbarSize="50dip">




        <!-- Parent layout starts -->
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:background="#F1F1F1"
            android:orientation="vertical">

            <LinearLayout
                android:layout_width="fill_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="10dp"
                >
                <ImageView
                    android:layout_width="64dp"
                    android:layout_height="64dp"
                    android:src="@drawable/uncolor"
                    />
                <TextView
                    style="@style/TextView"
                    android:text="@string/lhwf1header_c"
                    android:layout_marginTop="20dp"
                    android:layout_marginLeft="7dp"
                    android:textColor="#008E45"

                    />

            </LinearLayout>
            <View
                android:layout_width="match_parent"
                android:layout_height="1dp"
                android:layout_marginLeft="10dp"
                android:layout_marginRight="10dp"
                android:layout_marginBottom="10dp"
                android:background="@color/hitnscolor" />









        <LinearLayout
            style="@style/Q_Linerlayout_v"
            android:id="@+id/SectionC"
            >






            <!-- lhwf2c1 starts -->
            <android.support.v7.widget.CardView style="@style/Q_CardView"


                >



                <LinearLayout
                    android:id="@+id/Lv_lhwf2c1"
                    style="@style/Q_Linerlayout_v"

                    >

                    <TextView

                        style="@style/TextView"
                        android:text="Is there any PW whose pregnancy was identified in last three months?"

                        />
                    <RadioGroup
                        android:id="@+id/lhwf2c1"
                        style="@style/RadioGroup">

                        <RadioButton
                            android:id="@+id/lhwf2c1_1"
                            style="@style/RadioButton"
                            android:text="@string/rb_yes"/>

                        <RadioButton
                            android:id="@+id/lhwf2c1_2"
                            style="@style/RadioButton"
                            android:text="@string/rb_no"/>


                    </RadioGroup>


                </LinearLayout>
            </android.support.v7.widget.CardView>
            <!-- lhwf2c1 ends-->



            <!-- lhwf2c2 starts -->
            <android.support.v7.widget.CardView style="@style/Q_CardView"
                >



                <LinearLayout
                    android:id="@+id/Lv_lhwf2c2"
                    style="@style/Q_Linerlayout_v"

                    >

                    <TextView

                        style="@style/TextView"
                        android:text="@string/lhwf2c2"

                        />

                    <EditText
                        style="@style/EditText_number"
                        android:id="@+id/lhwf2c2"
                        android:maxLength="3"
                        android:hint="@string/lhwf2c2"/>


                </LinearLayout>
            </android.support.v7.widget.CardView>
            <!-- lhwf2c2 ends-->


            <!-- lhwf2c3 starts -->
            <android.support.v7.widget.CardView style="@style/Q_CardView"


                >



                <LinearLayout
                    android:id="@+id/Lv_lhwf2c3"
                    style="@style/Q_Linerlayout_v"

                    >

                    <TextView

                        style="@style/TextView"
                        android:text="@string/lhwf2c3"

                        />

                    <EditText
                        style="@style/EditText_Name"
                        android:id="@+id/lhwf2c3"
                        android:maxLength="50"
                        android:hint="@string/lhwf2c3"/>


                </LinearLayout>
            </android.support.v7.widget.CardView>
            <!-- lhwf2c3 ends-->



            <!-- lhwf2c4 starts -->
            <android.support.v7.widget.CardView style="@style/Q_CardView"


                >



                <LinearLayout
                    android:id="@+id/Lv_lhwf2c4"
                    style="@style/Q_Linerlayout_v"

                    >

                    <TextView

                        style="@style/TextView"
                        android:text="@string/lhwf2c4"

                        />

                    <EditText
                        style="@style/EditText_Name"
                        android:id="@+id/lhwf2c4"
                        android:maxLength="50"
                        android:hint="@string/lhwf2c4"/>


                </LinearLayout>
            </android.support.v7.widget.CardView>
            <!-- lhwf2c4 ends-->


            <!-- lhwf2c4a starts -->
            <android.support.v7.widget.CardView style="@style/Q_CardView"


                >



                <LinearLayout
                    android:id="@+id/Lv_lhwf2c4a"
                    style="@style/Q_Linerlayout_v"

                    >

                    <TextView

                        style="@style/TextView"
                        android:text="Did LHW visited you during pregnancy?"

                        />
                    <RadioGroup
                        android:id="@+id/lhwf2c4a"
                        style="@style/RadioGroup">

                        <RadioButton
                            android:id="@+id/lhwf2c4a_1"
                            style="@style/RadioButton"
                            android:text="@string/rb_yes"/>

                        <RadioButton
                            android:id="@+id/lhwf2c4a_2"
                            style="@style/RadioButton"
                            android:text="@string/rb_no"/>


                    </RadioGroup>


                </LinearLayout>
            </android.support.v7.widget.CardView>
            <!-- lhwf2c4a ends-->



            <!-- lhwf2c5 starts -->
