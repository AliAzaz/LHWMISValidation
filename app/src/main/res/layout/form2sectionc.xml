                    <TextView

                        style="@style/TextView"
                        android:text="@string/lhwf2c5"

                        />

                    <EditText
                        style="@style/EditText_number"
                        android:id="@+id/lhwf2c5"
                        android:maxLength="1"
                        android:hint="@string/lhwf2c5"/>


                </LinearLayout>
            </android.support.v7.widget.CardView>
            <!-- lhwf2c5 ends-->



            <!-- lhwf2c6 starts -->
            <android.support.v7.widget.CardView style="@style/Q_CardView"


                >



                <LinearLayout
                    android:id="@+id/Lv_lhwf2c6"
                    style="@style/Q_Linerlayout_v"

                    >

                    <TextView

                        style="@style/TextView"
                        android:text="@string/lhwf2c6"

                        />
                    <RadioGroup

                        android:id="@+id/lhwf2c6"
                        style="@style/RadioGroup">

                        <RadioButton
                            android:id="@+id/lhwf2c6_1"
                            style="@style/RadioButton"
                            android:text="@string/rb_yes"/>

                        <RadioButton
                            android:id="@+id/lhwf2c6_2"
                            style="@style/RadioButton"
                            android:text="@string/rb_no"/>

                        <RadioButton
                            android:id="@+id/lhwf2c6_3"
                            style="@style/RadioButton"
                            android:text="@string/rb_na"/>

                    </RadioGroup>


                </LinearLayout>
            </android.support.v7.widget.CardView>
            <!-- lhwf2c6 ends-->


            <!-- lhwf2c7 starts -->
            <android.support.v7.widget.CardView style="@style/Q_CardView"

                android:visibility='@{lhwf2c61.checked? View.GONE : View.VISIBLE}'
                >



                <LinearLayout
                    android:id="@+id/Lv_lhwf2c7"
                    style="@style/Q_Linerlayout_v"

                    >

                    <TextView

                        style="@style/TextView"
                        android:text="@string/lhwf2c7"

                        />
                    <RadioGroup
                        android:id="@+id/lhwf2c7"
                        style="@style/RadioGroup">

                        <RadioButton
                            android:id="@+id/lhwf2c7_1"
                            style="@style/RadioButton"
                            android:text="@string/rb_yes"/>

                        <RadioButton
                            android:id="@+id/lhwf2c7_2"
                            style="@style/RadioButton"
                            android:text="@string/rb_no"/>



                    </RadioGroup>


                </LinearLayout>
            </android.support.v7.widget.CardView>
            <!-- lhwf2c7 ends-->

            <!-- lhwf2c8 starts -->
            <android.support.v7.widget.CardView style="@style/Q_CardView"

                android:visibility='@{lhwf2c61.checked? View.GONE : View.VISIBLE}'
                >



                <LinearLayout
                    android:id="@+id/Lv_lhwf2c8"
                    style="@style/Q_Linerlayout_v"

                    >

                    <TextView

                        style="@style/TextView"
                        android:text="@string/lhwf2c8"

