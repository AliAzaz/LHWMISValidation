<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    tools:context=".LHWDashbord">

    <data>

        <import type="android.view.View" />

        <variable
            name="callback"
            type="com.irfansyed.umeedenau.validation.Form1SectionG" />
    </data>


    <ScrollView
        android:layout_width="match_parent"
        android:layout_height="fill_parent"
        android:layout_marginBottom="5dp"
        android:fadeScrollbars="false"
        android:fillViewport="true"
        android:paddingLeft="5dp"
        android:paddingTop="5dp"
        android:paddingRight="5dp"
        android:paddingBottom="5dp"
        android:scrollbarSize="50dip">

        <!-- Parent layout starts -->
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:background="#F1F1F1"
            android:orientation="vertical">

            <LinearLayout
                android:layout_width="fill_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="10dp">

                <ImageView
                    android:layout_width="64dp"
                    android:layout_height="64dp"
                    android:src="@drawable/uncolor" />

                <TextView
                    style="@style/TextView"
                    android:layout_marginLeft="7dp"
                    android:layout_marginTop="20dp"
                    android:text="@string/lhwf1header_g"
                    android:textColor="#008E45" />

            </LinearLayout>

            <View
                android:layout_width="match_parent"
                android:layout_height="1dp"
                android:layout_marginLeft="10dp"
                android:layout_marginRight="10dp"
                android:layout_marginBottom="10dp"
                android:background="@color/hitnscolor" />


            <LinearLayout
                android:id="@+id/SectionG"
                style="@style/Q_Linerlayout_v">


                <!-- lhwf1g1 starts -->
                <androidx.cardview.widget.CardView
                    android:id="@+id/cvlhwf1g1"
                    style="@style/Q_CardView"
                    android:visibility="gone">

                    <LinearLayout
                        android:id="@+id/Lv_lhwf1g1"
                        style="@style/Q_Linerlayout_v">

                        <TextView
                            style="@style/TextView"
                            android:text="@string/lhwf1g1" />

                        <com.edittextpicker.aliazaz.EditTextPicker
                            android:id="@+id/lhwf1g1"
                            style="@style/EditText_number"
                            android:hint="@string/num"
                            android:maxLength="3"
                            app:maxValue="150"
                            app:minValue="1"
                            app:type="range" />

                    </LinearLayout>

                </androidx.cardview.widget.CardView>
                <!-- lhwf1g1 ends-->


                <!-- lhwf1g2 starts -->
                <androidx.cardview.widget.CardView
                    android:id="@+id/cvlhwf1g2"
                    style="@style/Q_CardView">

                    <LinearLayout
                        android:id="@+id/Lv_lhwf1g2"
                        style="@style/Q_Linerlayout_v">

                        <TextView
                            style="@style/TextView"
                            android:text="@string/lhwf1g2" />

                        <com.edittextpicker.aliazaz.EditTextPicker
                            android:id="@+id/lhwf1g2"
                            style="@style/EditText_number"
                            android:hint="@string/num"
                            android:maxLength="3"
                            app:maxValue="150"
                            app:minValue="1"
                            app:type="range" />

                    </LinearLayout>

                </androidx.cardview.widget.CardView>
                <!-- lhwf1g2 ends-->


                <!-- lhwf1g3 starts -->
                <androidx.cardview.widget.CardView
                    android:id="@+id/cvlhwf1g3"
                    style="@style/Q_CardView">

                    <LinearLayout
                        android:id="@+id/Lv_lhwf1g3"
                        style="@style/Q_Linerlayout_v">

                        <TextView
                            style="@style/TextView"
                            android:text="@string/lhwf1g3" />

                        <com.edittextpicker.aliazaz.EditTextPicker
                            android:id="@+id/lhwf1g3"
                            style="@style/EditText_number"
                            android:hint="@string/num"
                            android:maxLength="2"
                            app:maxValue="20"
                            app:minValue="1"
                            app:type="range" />

                    </LinearLayout>

                </androidx.cardview.widget.CardView>
                <!-- lhwf1g3 ends-->


                <!-- lhwf1g4 starts -->
                <androidx.cardview.widget.CardView
                    android:id="@+id/cvlhwf1g4"
                    style="@style/Q_CardView">

                    <LinearLayout
                        android:id="@+id/Lv_lhwf1g4"
                        style="@style/Q_Linerlayout_v">

                        <TextView
                            style="@style/TextView"
                            android:text="@string/lhwf1g4" />

                        <EditText
                            android:id="@+id/lhwf1g4"
                            style="@style/EditText_Name"
                            android:hint="@string/name"
                            android:maxLength="50" />

                    </LinearLayout>

                </androidx.cardview.widget.CardView>
                <!-- lhwf1g4 ends-->


                <!-- lhwf1g5 starts -->
                <androidx.cardview.widget.CardView
                    android:id="@+id/cvlhwf1g5"
                    style="@style/Q_CardView">

                    <LinearLayout
                        android:id="@+id/Lv_lhwf1g5"
                        style="@style/Q_Linerlayout_v">

                        <TextView
                            style="@style/TextView"
                            android:text="@string/lhwf1g5" />

                        <com.edittextpicker.aliazaz.EditTextPicker
                            android:id="@+id/lhwf1g5"
                            style="@style/EditText_number"
                            android:hint="@string/months"
                            android:maxLength="2"
                            app:maxValue="59"
                            app:minValue="0"
                            app:type="range" />

                    </LinearLayout>

                </androidx.cardview.widget.CardView>
                <!-- lhwf1g5 ends-->


                <!-- lhwf1g10a  starts -->
                <androidx.cardview.widget.CardView
                    android:id="@+id/cvlhwf1g10a"
                    style="@style/Q_CardView">

                    <LinearLayout
                        android:id="@+id/Lv_lhwf1g10a"
                        style="@style/Q_Linerlayout_v">

                        <TextView
                            style="@style/TextView"
                            android:text="@string/lhwf1g10a" />

                        <RadioGroup
                            android:id="@+id/lhwf1g10a"
                            style="@style/RadioGroup"
                            android:orientation="horizontal">

                            <RadioButton
                                android:id="@+id/lhwf1g10a_1"
                                style="@style/RadioButton"
                                android:layout_width="0dp"
                                android:layout_weight="1"
                                android:text="@string/rb_yes" />

                            <RadioButton
                                android:id="@+id/lhwf1g10a_2"
                                style="@style/RadioButton"
                                android:layout_width="0dp"
                                android:layout_weight="1"
                                android:text="@string/rb_no" />

                        </RadioGroup>

                    </LinearLayout>

                </androidx.cardview.widget.CardView>
                <!-- lhwf1g10a ends-->


                <!-- lhwf1g10b  starts -->
                <androidx.cardview.widget.CardView
                    android:id="@+id/cvlhwf1g10b"
                    style="@style/Q_CardView">

                    <LinearLayout
                        android:id="@+id/Lv_lhwf1g10b"
                        style="@style/Q_Linerlayout_v">

                        <TextView
                            style="@style/TextView"
                            android:text="@string/lhwf1g10b" />

                        <RadioGroup
                            android:id="@+id/lhwf1g10b"
                            style="@style/RadioGroup">

                            <RadioButton
                                android:id="@+id/lhwf1g10b_1"
                                style="@style/RadioButton"
                                android:text="@string/lhwf1g10b_1" />

                            <RadioButton
                                android:id="@+id/lhwf1g10b_2"
                                style="@style/RadioButton"
                                android:text="@string/lhwf1g10b_2" />

                            <RadioButton
                                android:id="@+id/lhwf1g10b_3"
                                style="@style/RadioButton"
                                android:text="@string/lhwf1g10b_3" />

                        </RadioGroup>

                    </LinearLayout>

                </androidx.cardview.widget.CardView>
                <!-- lhwf1g10b ends-->


                <!-- lhwf1g4A starts -->
                <androidx.cardview.widget.CardView
                    android:id="@+id/cvlhwf1g4A"
                    style="@style/Q_CardView">

                    <LinearLayout
                        android:id="@+id/Lv_lhwf1g4A"
                        style="@style/Q_Linerlayout_v">

                        <TextView
                            style="@style/TextView"
                            android:text="@string/lhwf1g4A" />

                        <LinearLayout
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:orientation="horizontal">

                            <com.edittextpicker.aliazaz.EditTextPicker
                                android:id="@+id/lhwf1g4Aa"
                                style="@style/EditText_number"
                                android:layout_width="0dp"
                                android:layout_weight="1"
                                android:hint="@string/months"
                                android:maxLength="2"
                                app:maxValue="12"
                                app:minValue="1"
                                app:type="range" />

                            <com.edittextpicker.aliazaz.EditTextPicker
                                android:id="@+id/lhwf1g4Ab"
                                style="@style/EditText_number"
                                android:layout_width="0dp"
                                android:layout_weight="1"
                                android:hint="@string/date"
                                android:maxLength="2"
                                app:maxValue="31"
                                app:minValue="1"
                                app:type="range" />

                        </LinearLayout>

                    </LinearLayout>

                </androidx.cardview.widget.CardView>
                <!-- lhwf1g4A ends-->


                <!-- lhwf1g6 starts -->
                <androidx.cardview.widget.CardView
                    android:id="@+id/cvlhwf1g6"
                    style="@style/Q_CardView">

                    <LinearLayout
                        android:id="@+id/Lv_lhwf1g6"
                        style="@style/Q_Linerlayout_v">

                        <TextView
                            style="@style/TextView"
                            android:text="@string/lhwf1g6" />

                        <RadioGroup
                            android:id="@+id/lhwf1g6"
                            style="@style/RadioGroup"
                            android:orientation="horizontal">

                            <RadioButton
                                android:id="@+id/lhwf1g6_1"
                                style="@style/RadioButton"
                                android:layout_width="0dp"
                                android:layout_weight="1"
                                android:text="@string/rb_yes" />

                            <RadioButton
                                android:id="@+id/lhwf1g6_2"
                                style="@style/RadioButton"
                                android:layout_width="0dp"
                                android:layout_weight="1"
                                android:text="@string/rb_no" />

                            <RadioButton
                                android:id="@+id/lhwf1g6_3"
                                style="@style/RadioButton"
                                android:layout_width="0dp"
                                android:layout_weight="1"
                                android:text="@string/lhw_stockout" />

                        </RadioGroup>

                    </LinearLayout>

                </androidx.cardview.widget.CardView>
                <!-- lhwf1g6 ends-->


                <!-- lhwf1g7 starts -->
                <androidx.cardview.widget.CardView
                    android:id="@+id/cvlhwf1g7"
                    style="@style/Q_CardView">

                    <LinearLayout
                        android:id="@+id/Lv_lhwf1g7"
                        style="@style/Q_Linerlayout_v">

                        <TextView
                            style="@style/TextView"
                            android:text="@string/lhwf1g7" />

                        <RadioGroup
                            android:id="@+id/lhwf1g7"
                            style="@style/RadioGroup"
                            android:orientation="horizontal">

                            <RadioButton
                                android:id="@+id/lhwf1g7_1"
                                style="@style/RadioButton"
                                android:layout_width="0dp"
                                android:layout_weight="1"
                                android:text="@string/rb_yes" />

                            <RadioButton
                                android:id="@+id/lhwf1g7_2"
                                style="@style/RadioButton"
                                android:layout_width="0dp"
                                android:layout_weight="1"
                                android:text="@string/rb_no" />

                        </RadioGroup>

                    </LinearLayout>

                </androidx.cardview.widget.CardView>
                <!-- lhwf1g7 ends-->


                <!-- lhwf1g8 starts -->
                <androidx.cardview.widget.CardView
                    android:id="@+id/cvlhwf1g8"
                    style="@style/Q_CardView">

                    <LinearLayout
                        android:id="@+id/Lv_lhwf1g8"
                        style="@style/Q_Linerlayout_v">

                        <TextView
                            style="@style/TextView"
                            android:text="@string/lhwf1g8" />

                        <RadioGroup
                            android:id="@+id/lhwf1g8"
                            style="@style/RadioGroup"
                            android:orientation="horizontal">

                            <RadioButton
                                android:id="@+id/lhwf1g8_1"
                                style="@style/RadioButton"
                                android:layout_width="0dp"
                                android:layout_weight="1"
                                android:text="@string/rb_yes" />

                            <RadioButton
                                android:id="@+id/lhwf1g8_2"
                                style="@style/RadioButton"
                                android:layout_width="0dp"
                                android:layout_weight="1"
                                android:text="@string/rb_no" />

                        </RadioGroup>

                    </LinearLayout>

                </androidx.cardview.widget.CardView>
                <!-- lhwf1g8 ends-->


                <!-- lhwf1g9 starts -->
                <androidx.cardview.widget.CardView
                    android:id="@+id/cvlhwf1g9"
                    style="@style/Q_CardView">

                    <LinearLayout
                        android:id="@+id/Lv_lhwf1g9"
                        style="@style/Q_Linerlayout_v">

                        <TextView
                            style="@style/TextView"
                            android:text="@string/lhwf1g9" />

                        <RadioGroup
                            android:id="@+id/lhwf1g9"
                            style="@style/RadioGroup"
                            android:orientation="horizontal">

                            <RadioButton
                                android:id="@+id/lhwf1g9_1"
                                style="@style/RadioButton"
                                android:layout_width="0dp"
                                android:layout_weight="1"
                                android:text="@string/rb_yes" />

                            <RadioButton
                                android:id="@+id/lhwf1g9_2"
                                style="@style/RadioButton"
                                android:layout_width="0dp"
                                android:layout_weight="1"
                                android:text="@string/rb_no" />

                        </RadioGroup>

                    </LinearLayout>

                </androidx.cardview.widget.CardView>
                <!-- lhwf1g9 ends-->
                <!-- Button -->
                <androidx.cardview.widget.CardView style="@style/Q_CardView">

                    <LinearLayout style="@style/Q_Linerlayout_v">

                        <EditText
                            android:id="@+id/lhwf1gphoto"
                            android:layout_width="match_parent"
                            android:layout_height="wrap_content"
                            android:enabled="false" />

                        <Button
                            android:id="@+id/btn_snap"
                            android:layout_width="wrap_content"
                            android:layout_height="wrap_content"
                            android:text="Snap Shot" />


                    </LinearLayout>

                </androidx.cardview.widget.CardView>

            </LinearLayout>


            <!-- Button Next Starts-->
            <androidx.cardview.widget.CardView style="@style/Q_CardView">

                <Button
                    android:id="@+id/btn_next"
                    style="@style/Button_Next"
                    android:layout_margin="10dp"
                    android:layout_weight="50"
                    android:text="Submit " />

            </androidx.cardview.widget.CardView>
            <!-- Button Next Ends-->


        </LinearLayout>

    </ScrollView>


</layout>
